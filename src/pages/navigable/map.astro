---
import Layout from "../../layouts/Layout.astro";
import MapComponent from "../../components/MapComponent";

---

<Layout title="Professionisti Formati"
    description="Consulta la lista dei professionisti formati dalla Dottoressa Liana Messini durante i corsi di Formazione"
    canonical="https://studiomessini.com/navigable/map"
    ogImage="https://studiomessini.com/logo.webp"
    robots="noindex, nofollow"
>
    <div class="text-center mt-10 mb-10">
        <h1 class="text-3xl lg:text-4xl font-extrabold">Ho Formato</h1>
        <h2 class="text-xl lg:text-2xl font-medium mt-2">qui puoi consultare una lista dei discenti<br>
            che ho formato recentemente</h2>
        <h3 class="text text-sm mt-4">(NOTA: dopo aver selezionato un filtro premi per applicarlo)</h3>
    </div>
    <MapComponent client:visible />
</Layout>
